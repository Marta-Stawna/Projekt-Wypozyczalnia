{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
          "modules": true,
          "experimentalObjectRestSpread": true
      }
  },
  "plugins": [
      "react"
  ],
  "extends": ["eslint:all", "plugin:react/all"],
  "settings": {
      "react": {
          "pragma": "React",
          "version": "15.4.2"
      }
  }
}
